{
  "version": 2,
  "rewrites": [
    {
      "source": "/send-email",
      "destination": "./server/app.js"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/send-email",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
